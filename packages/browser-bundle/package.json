{
    "name": "@node-wot/browser-bundle",
    "version": "0.8.0",
    "description": "A node-wot bundle that can run in a web browser",
    "author": "Eclipse Thingweb <thingweb-dev@eclipse.org> (https://thingweb.io/)",
    "license": "EPL-2.0 OR W3C-20150513",
    "repository": "https://github.com/eclipse/thingweb.node-wot/tree/master/packages/browser-bundle",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/"
    ],
    "main": "dist/wot-bundle.min.js",
    "devDependencies": {
        "@node-wot/binding-http": "0.8.0",
        "@node-wot/binding-websockets": "0.8.0",
        "@node-wot/core": "0.8.0",
        "browserify": "16.5.0",
        "tinyify": "2.5.2",
        "typescript": "4.4.3",
        "wot-typescript-definitions": "^0.8.0-SNAPSHOT.18"
    },
    "scripts": {
        "build": "browserify -r vm:vm2 index.js --plugin tinyify --external coffee-script -o dist/wot-bundle.min.js",
        "lint": "eslint .",
        "format": "prettier --write index.js \"**/*.json\""
    },
    "bugs": {
        "url": "https://github.com/eclipse/thingweb.node-wot/issues"
    },
    "homepage": "https://github.com/eclipse/thingweb.node-wot/tree/master/packages/browser-bundle#readme",
    "keywords": []
}
